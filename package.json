{
  "name": "bowlingball",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "./bin/release",
    "build:app": "tsc && vite build && ./bin/compat",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
